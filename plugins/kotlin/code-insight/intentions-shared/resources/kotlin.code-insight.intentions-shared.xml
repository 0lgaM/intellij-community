<idea-plugin package="org.jetbrains.kotlin.idea.codeInsight.intentions.shared">
  <extensions defaultExtensionNs="com.intellij">
    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.AddBracesIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.AddBracesToAllBranchesIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.ConvertToForEachFunctionCallIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.RemoveBracesIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.RemoveBracesFromAllBranchesIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.RemoveUnnecessaryParenthesesIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.AddUnderscoresToNumericLiteralIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.RemoveUnderscoresFromNumericLiteralIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

    <intentionAction>
      <language>kotlin</language>
      <className>org.jetbrains.kotlin.idea.codeInsight.intentions.shared.SwapBinaryExpressionIntention</className>
      <bundleName>messages.KotlinBundle</bundleName>
      <categoryKey>group.names.kotlin</categoryKey>
    </intentionAction>

  </extensions>
</idea-plugin>