<idea-plugin>
  <extensions defaultExtensionNs="org.jetbrains.kotlin.idea.compilerPlugin.parcelize">
    <availabilityProvider implementation="org.jetbrains.kotlin.idea.gradleJava.compilerPlugin.parcelize.GradleParcelizeAvailabilityProvider"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij">
    <externalProjectDataService implementation="org.jetbrains.kotlin.idea.gradleJava.compilerPlugin.parcelize.ParcelizeIdeModelDataService"/>
  </extensions>

  <extensions defaultExtensionNs="org.jetbrains.plugins.gradle">
    <projectResolve implementation="org.jetbrains.kotlin.idea.gradleJava.compilerPlugin.parcelize.ParcelizeProjectResolverExtension" order="last"/>
  </extensions>
</idea-plugin>
