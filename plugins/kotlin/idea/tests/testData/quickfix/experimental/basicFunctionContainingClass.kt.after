// "Propagate 'MyExperimentalAPI' opt-in requirement to containing class 'Bar'" "true"
// COMPILER_ARGUMENTS: -opt-in=kotlin.RequiresOptIn
// WITH_RUNTIME

@RequiresOptIn
annotation class MyExperimentalAPI

@MyExperimentalAPI
fun foo() {}

@MyExperimentalAPI
class Bar {
    fun bar() {
        foo()
    }
}
