<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.javascript.web.framework"
                    beanClass="com.intellij.webSymbols.framework.impl.WebFrameworkRegistrationExtensionPoint"
                    dynamic="true">
      <with attribute="implementation" implements="com.intellij.webSymbols.framework.WebFramework"/>
    </extensionPoint>
    <extensionPoint qualifiedName="com.intellij.javascript.web.context"
                    beanClass="com.intellij.webSymbols.framework.impl.WebFrameworkExtensionPoint"
                    dynamic="true">
      <with attribute="implementation" implements="com.intellij.webSymbols.framework.WebFrameworkContext"/>
    </extensionPoint>
    <!--suppress PluginXmlValidity -->
    <extensionPoint qualifiedName="com.intellij.javascript.webTypes"
                    beanClass="com.intellij.webSymbols.webTypes.WebTypesDefinitionsEP"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.webTypes.symbolTypeResolverFactory"
                    beanClass="com.intellij.webSymbols.webTypes.WebTypesSymbolTypeResolverFactoryEP"
                    dynamic="true">
      <with attribute="implementation" implements="com.intellij.webSymbols.webTypes.WebTypesSymbolTypeResolver$Factory"/>
    </extensionPoint>
    <extensionPoint qualifiedName="com.intellij.javascript.web.additionalContextProvider"
                    interface="com.intellij.webSymbols.WebSymbolsAdditionalContextProvider"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.filter"
                    beanClass="com.intellij.webSymbols.impl.WebSymbolsFilterEP"
                    dynamic="true">
      <with attribute="implementation" implements="com.intellij.webSymbols.WebSymbolsFilter"/>
    </extensionPoint>
    <extensionPoint qualifiedName="com.intellij.javascript.web.documentationCustomizer"
                    interface="com.intellij.webSymbols.WebSymbolDocumentationCustomizer"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.codeCompletionItemCustomizer"
                    interface="com.intellij.webSymbols.WebSymbolCodeCompletionItemCustomizer"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.defaultIconProvider"
                    interface="com.intellij.webSymbols.WebSymbolDefaultIconProvider"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.scopeProvider"
                    interface="com.intellij.webSymbols.WebSymbolsScopeProvider"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.declarationProvider"
                    interface="com.intellij.webSymbols.WebSymbolDeclarationProvider"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.psiSourcedSymbolProvider"
                    interface="com.intellij.webSymbols.PsiSourcedWebSymbolProvider"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.psiSourcedSymbol"
                    beanClass="com.intellij.webSymbols.findUsages.impl.WebSymbolPsiSourcedSymbolHostClassEP"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.inspectionToolMapping"
                    beanClass="com.intellij.webSymbols.inspections.impl.WebSymbolsInspectionToolMappingEP"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.highlightInLanguage"
                    beanClass="com.intellij.webSymbols.inspections.impl.WebSymbolsHighlightInLanguageEP"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.javascript.web.dependencyProximityProvider"
                    interface="com.intellij.webSymbols.framework.DependencyProximityProvider"
                    dynamic="true"/>
  </extensionPoints>
</idea-plugin>
